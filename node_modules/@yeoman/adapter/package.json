    "ora": "^8.1.0",
    "p-queue": "^8.0.1",
    "text-table": "^0.2.0"
  },
  "devDependencies": {
    "@types/text-table": "^0.2.2",
    "strip-ansi": "^7.0.1",
    "yeoman-assert": "^3.1.1"
  },
  "engines": {
    "node": ">=18.12.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
